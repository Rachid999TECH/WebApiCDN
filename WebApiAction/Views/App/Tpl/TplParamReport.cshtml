@{
    ViewBag.ParamReportId = @ViewBag.Pattern + "ParamReportId";
    ViewBag.ParamCardId = @ViewBag.Pattern + "ParamCardId";
    ViewBag.ParamCardHeaderId = @ViewBag.Pattern + "ParamCardHeaderId";
    ViewBag.ParamTitleId = @ViewBag.Pattern + "ParamTitleId";
    ViewBag.ParamCardBodyId = @ViewBag.Pattern + "ParamCardBodyId";
    ViewBag.ParamHeaderId = @ViewBag.Pattern + "ParamHeaderId";
    ViewBag.ParamRowId = @ViewBag.Pattern + "ParamRowId";
    ViewBag.ParamCol0Id = @ViewBag.Pattern + "ParamCol0Id";
    ViewBag.ParamCol1Id = @ViewBag.Pattern + "ParamCol1Id";

    ViewBag.ParamActionContainerId = @ViewBag.Pattern + "ParamActionContainerId";
    ViewBag.ParamActionIconId = @ViewBag.Pattern + "ParamActionIconId";
    ViewBag.ParamActionBtnId = @ViewBag.Pattern + "ParamActionBtnId";
    ViewBag.ParamActionTextId = @ViewBag.Pattern + "ParamActionTextId";

    ViewBag.ParamActionContainerId1 = @ViewBag.Pattern + "ParamActionContainerId1";
    ViewBag.ParamActionIconId1 = @ViewBag.Pattern + "ParamActionIconId1";
    ViewBag.ParamActionBtnId1 = @ViewBag.Pattern + "ParamActionBtnId1";
    ViewBag.ParamActionTextId1 = @ViewBag.Pattern + "ParamActionTextId1";

    ViewBag.ParamActionContainerId2 = @ViewBag.Pattern + "ParamActionContainerId2";
    ViewBag.ParamActionIconId2 = @ViewBag.Pattern + "ParamActionIconId2";
    ViewBag.ParamActionBtnId2 = @ViewBag.Pattern + "ParamActionBtnId2";
    ViewBag.ParamActionTextId2 = @ViewBag.Pattern + "ParamActionTextId2";

    ViewBag.ParamActionContainerId3 = @ViewBag.Pattern + "ParamActionContainerId3";
    ViewBag.ParamActionIconId3 = @ViewBag.Pattern + "ParamActionIconId3";
    ViewBag.ParamActionBtnId3 = @ViewBag.Pattern + "ParamActionBtnId3";
    ViewBag.ParamActionTextId3 = @ViewBag.Pattern + "ParamActionTextId3";

    ViewBag.ParamMessageContainerId = @ViewBag.Pattern + "ParamMessageContainerId";
    ViewBag.ParamMessageIconId = @ViewBag.Pattern + "ParamMessageIconId";
    ViewBag.ParamMessageTextId = @ViewBag.Pattern + "ParamMessageTextId";

    
}

<div id="@ViewBag.ParamReportId" class="row mb-3" style="max-width: 915px;margin-left: auto;margin-right: auto;">
    <div class="col-md-12">
        <div id="@ViewBag.ParamCardId" class="card box-cl">
            <div id="@ViewBag.ParamCardHeaderId" class="card-header">
                <h5 id="@ViewBag.ParamTitleId" class="panel-title">@ViewBag.ParamTitle</h5>
            </div>
            <div id="@ViewBag.ParamCardBodyId" class="card-body">
                <div id="@ViewBag.ParamHeaderId"></div>
                <div id="@ViewBag.ParamRowId" class="row">
                    <div id="@ViewBag.ParamCol0Id" class="col param-col"></div>
                    <div id="@ViewBag.ParamCol1Id" class="col param-col"></div>
                </div>
                <div class="row">
                    <div id="ParamCol0Id" class="col-sm-6"></div>
                </div>
                <div id="ParamLigne0Id"></div>
                <div id="@ViewBag.ParamActionContainerId" class="text-left">
                    <button id="@ViewBag.ParamActionBtnId" type="button" class="btn-cl gc-bc1-cl" onclick="@ViewBag.Action">
                        <span id="@ViewBag.ParamActionIconId" class="@ViewBag.ActionIcon btn-icon-m-cl gc-c1-cl"></span>
                        <div id="@ViewBag.ParamActionTextId" class="btn-txt-cl gc-c1-cl">@ViewBag.ActionText</div>
                    </button>
                </div>

                <div id="@ViewBag.ParamActionContainerId1" class="text-left">
                    <button id="@ViewBag.ParamActionBtnId1" type="button" class="btn-cl gc-bc1-cl" onclick="@ViewBag.Action1">
                        <span id="@ViewBag.ParamActionIconId1" class="@ViewBag.ActionIcon1 btn-icon-m-cl gc-c1-cl"></span>
                        <div id="@ViewBag.ParamActionTextId1" class="btn-txt-cl gc-c1-cl">@ViewBag.ActionText1</div>
                    </button>
                </div>

                <div id="@ViewBag.ParamActionContainerId2" class="text-left">
                    <button id="@ViewBag.ParamActionBtnId2" type="button" class="btn-cl gc-bc1-cl" onclick="@ViewBag.Action2">
                        <span id="@ViewBag.ParamActionIconId2" class="@ViewBag.ActionIcon2 btn-icon-m-cl gc-c1-cl"></span>
                        <div id="@ViewBag.ParamActionTextId2" class="btn-txt-cl gc-c1-cl">@ViewBag.ActionText2</div>
                    </button>
                </div>

                <div id="@ViewBag.ParamActionContainerId3" class="text-left">
                    <button id="@ViewBag.ParamActionBtnId3" type="button" class="btn-cl gc-bc1-cl" onclick="@ViewBag.Action3">
                        <span id="@ViewBag.ParamActionIconId3" class="@ViewBag.ActionIcon3 btn-icon-m-cl gc-c1-cl"></span>
                        <div id="@ViewBag.ParamActionTextId3" class="btn-txt-cl gc-c1-cl">@ViewBag.ActionText3</div>
                    </button>
                </div>

                

            </div>
            <div id="@ViewBag.ParamMessageContainerId" class="card-body" style="width: 100%; margin-left: 0px; display: unset; height: 30px; font-size: 14px;">
                <span id="ParamMessageIconId"></span>
                <span id="ParamMessageTextId"></span>
            </div>
            @*<div id="@ViewBag.ParamMessageContainerId" class="container" style="width: 100%;margin-left: 0px;display:none">
                    <span id="ParamMessageIconId"></span>
                    <span id="ParamMessageTextId"></span>
                </div>*@
        </div>
    </div>
</div>