<div id="@ViewBag.ParentId" class="form-group row mb-1">
    <label for="@ViewBag.Id" id="@ViewBag.LabelId" class="col-4 col-form-label" >@ViewBag.Label</label>
    <div class="col">
        <input class="form-control" type="text" id="@ViewBag.PlateOrderId"  onchange="@ViewBag.OnChange" style="height: 40px; border-color: red;">
    </div>
    <div class="col-2">
        <select class="form-control" id="@ViewBag.PlateSeriesId" onchange="@ViewBag.OnChange" style="height: 40px; border-color: red;"></select>
    </div>
    <div class="col">
        <input class="form-control" type="text" id="@ViewBag.PlatePrefectureId"  onchange="@ViewBag.OnChange" style="height: 40px; border-color: red;">
    </div>
</div>





