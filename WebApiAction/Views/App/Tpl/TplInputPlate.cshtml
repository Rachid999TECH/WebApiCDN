@{
    ViewBag.Id = "PlateId" + @ViewBag.FormIndex;
    ViewBag.ParentId = "PlateId" + @ViewBag.FormIndex + "Parent";
    ViewBag.LabelId = "PlateId" + @ViewBag.FormIndex + "Label";

    ViewBag.PlateOrderId = "PlateOrderId" + @ViewBag.FormIndex;
    ViewBag.PlateSeriesId = "PlateSeriesId" + @ViewBag.FormIndex;
    ViewBag.PlatePrefectureId = "PlatePrefectureId" + @ViewBag.FormIndex;
}
