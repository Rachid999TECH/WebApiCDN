@{
    ViewBag.IntroReportId = @ViewBag.Pattern + "IntroReportId";
    ViewBag.IntroCardId = @ViewBag.Pattern + "IntroCardId";
    ViewBag.IntroCardBodyId = @ViewBag.Pattern + "IntroCardBodyId";
    ViewBag.IntroRowId = @ViewBag.Pattern + "IntroRowId";
    ViewBag.IntroRow0Id = @ViewBag.Pattern + "IntroRow0Id";
    ViewBag.IntroRow1Id = @ViewBag.Pattern + "IntroRow1Id";
    ViewBag.IntroRow2Id = @ViewBag.Pattern + "IntroRow2Id";
    ViewBag.IntroRow2Id = @ViewBag.Pattern + "IntroRow2Id";

    ViewBag.IntroImageId = @ViewBag.Pattern + "IntroImageId";
    ViewBag.IntroIconValue = @ViewBag.IntroIcon + " tb-noti-cl gc-c1-cl";
}



<div id="@ViewBag.IntroReportId" class="row mb-3 intro-report">
    <div class="col-md-12 intro-container">
        <div id="@ViewBag.IntroCardId" class="card intro-card">
            <div id="@ViewBag.IntroCardBodyId" class="card-body intro-card-body">

                <div id="@ViewBag.IntroRow0Id" >
                    <img id="@ViewBag.IntroImageId" class="team-img-ct-cl center" src="~/Images/Profil/@ViewBag.ImageName">
                </div>

                <div id="@ViewBag.IntroRow1Id" class="team-desc-ct-cl">

                    <h4 class="mod-tl-cl">@ViewBag.IntroTitle</h4>
                    <p class="team-role-cl">@ViewBag.IntroRole</p>
                    <p class="team-desc-cl">@ViewBag.Description</p>


                    <ul class="social mb-0 list-inline mt-3">
                        <li class="list-inline-item"><a href="#" class="social-link"><i class="fab fa-facebook-f"></i></a></li>
                        <li class="list-inline-item"><a href="#" class="social-link"><i class="fab fa-twitter"></i></a></li>
                        <li class="list-inline-item"><a href="#" class="social-link"><i class="fab fa-instagram"></i></a></li>
                        <li class="list-inline-item"><a href="#" class="social-link"><i class="fab fa-linkedin-in"></i></a></li>
                    </ul>


                </div>

                <div id="@ViewBag.IntroRow2Id"></div>
                <div id="@ViewBag.IntroRow3Id"></div>
            </div>
        </div>
    </div>
</div>

