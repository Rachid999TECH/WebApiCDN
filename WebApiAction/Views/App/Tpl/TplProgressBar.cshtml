@{
    ViewBag.ParentId = @ViewBag.Id + "Parent";
    ViewBag.LabelId = @ViewBag.Id + "Label";
}

<div id="@ViewBag.ParentId" class="form-group row mb-1">
    <label for="@ViewBag.Id" id="@ViewBag.LabelId" class="col-4 col-form-label">@ViewBag.Label</label>
    <div class="col-8">
        <div class="progress">
            <div id="@ViewBag.Id" class="progress-bar progress-bar-striped " role="progressbar" style="width: 0%; background-color:#09c70d" aria-valuenow="10" aria-valuemin="0" aria-valuemax="100">
            </div>
        </div>
    </div>
</div>







